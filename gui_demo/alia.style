/* CRADLE SECTION - styling for content specific to the CRADLE-level UIs */

/* used to fill background space when the UI doesn't take up the full window */
background {
    background: #0e0e12;
}

/* background status overlay */

background-status-min {
    background: #26262af0;
    border-radius: 8px;
    margin: 8px;
}

background-status {
    background: #26262af0;
    border-radius: 8px;
    margin: 8px;
    width: 400px;
}
background-status/content {
    background: #0000;
}
background-status/heading {
    font-family: helvetica;
    font-size: 25;
    font-bold: true;
    color: #aaa;
}
background-status/job-count {
    font-family: helvetica;
    font-size: 16;
    font-bold: true;
    color: #88d;
}
background-status/job-type {
    font-family: arial;
    font-size: 15;
    font-bold: false;
    color: #aab;
}
background-status/transient-failure-count {
    font-family: helvetica;
    font-size: 14;
    font-bold: false;
    color: #cc8;
}
background-status/permanent-failure-count {
    font-family: helvetica;
    font-size: 14;
    font-bold: false;
    color: #d66;
}

animated-astroid {
    stroke-width: 0.4;
    stroke-length: 0.6;
    period: 4000;
    dark-time: 1400;
    segment-count: 20;
    color: #bbb;
    max-alpha: 0.8;
}

logo {
    color: #fff;
    height: 24px;
    scale: fit;
    alignment: left;
}

/* GENERAL SECTION - general styling info */

/ {
    default-padding: 4px;
    disable-padding: true;

    font-family: arial;
    font-size: 14;

    selected-color: #ddd;
    selected-background: #1059a4;

    focus-color: #369;
    focus-border-width: 1.3px;

    color: #999;
    background: #141418;

    /* This is the minimum width allocated to form labels.
       The default is small to avoid wasting space in the controls pane. */
    form-label-size: 6em 2em;
}

done-cancel-buttons {
}

done-cancel-buttons/button {
    color: #fff;
    border-radius: 3px;
    background: #cc6600;
}

done-cancel-buttons/button.hot {
    color: #fff;
    background: #ff9933;
}

close-buttons {
}

close-buttons/button {
    color: #fff;
    background: #660000;
    border-radius: 3px;
    margin: 0px 0px -5px;
}

close-buttons/button.hot {
    color: #fff;
    background: #990000;
}

close-buttons/button.depressed {
    color: #fff;
    background: #330000;
}

button {
    background: #336699;
    font-size: 14;
    padding: -2px 4px;
    border-radius: 3px;
    color: #bbb;
}
button.hot {
    background: #548dc6;
}
button.depressed {
    background: #406080;
}
button.disabled {
    background: #404040;
}

/* general-purpose content */
content {
    background: #141418;
    default-padding: 4px 4px;
}

/* main header */

header {
    default-padding: 4px 6px;
    background: #1d1e1e;
    color: #999;
    height: 55px;
}
header/separator {
    color: #333;
}
header/active-task-group {
    font-family: arial;
    font-size: 18;
    font-bold: true;
    color: #aaa;
}
header/task-group {
    font-family: arial;
    font-size: 16;
    font-bold: true;
}
header/task-group.hot {
    background: #28282c;
}
header/task-group.depressed {
    background: #303034;
}
header/title {
    font-family: Gotham SSm A;
    font-size: 20;
    font-bold: true;
    color: #ccc;
}
header/icon-row {
    default-padding: 4px;
}
header/icon-row/icon-button {
    size: 15px 15px;
    descent: 2px;
    border-width: 2px;
    border-color: #0000;
    border-radius: 0px;
    background: #0000;
    color: #bbb;
}
header/icon-row/icon-button.hot {
    border-color: #222;
    background: #222;
    color: #ddd;
}
header/icon-row/icon-button.depressed {
    border-color: #444;
    background: #444;
    color: #ddd;
}

header/realm {
    font-family: arial;
    font-size: 14;
    color: #79878d;
}

header/user {
    font-family: Gotham SSm A;
    font-size: 15;
    color: #bbb;
}

header/drop-down {
    default-padding: 2px;
    background: #202024;
}
header/drop-down.hot {
    default-padding: 2px;
    background: #202024;
}
header/drop-down.depressed {
    default-padding: 2px;
    background: #202024;
}
header/drop-down-list {
    background: #244061;
}
header/drop-down-list/item {
    default-padding: 4px;
    background: #244061;
    padding: 4px;
    font-family: arial;
    font-size: 14;
    color: #aaa; 
}
header/drop-down-list/item.hot {
    background: #336699;
}
header/drop-down-list/item.depressed {
    background: #48484c;
}
header/drop-down-list/item.selected {
    background: #b8cce4;
    color: #222;
}
header/drop-down-list/disabled-item : drop-down-list/item {
    background: #244061;
    color: #888; 
}

/* general-purpose headings */

section-heading {
    color: #bfa681;
    font-family: georgia;
    font-size: 16;
    font-bold: true;
}

subsection-heading {
    color: #a0a6ac;
    font-family: georgia;
    font-size: 14;
    font-bold: true;
}

form-section-heading : subsection-heading {
    margin: 12px 0px 2px;
}

/* standard UI widgets */

dashboard/separator {
    color: #555;
    width: 1px;
}

dashboard-header {
    font-bold: true;
    font-size: 16;
    margin: -8px 0px -8px -7px;
}

dashboard-item {
    margin: -2px 0px -2px 0px;
}
dashboard-item.hot {
    background: #313b44;
}
dashboard-item.depressed {
    background: #336699;
}
dashboard-item.selected {
    background: #336699;
}

minor-buttons/button {
    background: #456;
    font-size: 14;
    padding: -2px 4px;
    color: #bbb;
}
minor-buttons/button.hot {
    background: #567;
}
minor-buttons/button.depressed {
    background: #789;
}
minor-buttons/button.disabled {
    background: #404040;
}

color-chooser {
    default-padding: 2px 2px;
    padding: 2px 2px;
    border-radius: 4px;
}
color-chooser.hot {
    background: #202024;
}
color-chooser.depressed {
    background: #303034;
}
color-chooser.selected {
    background: #444;
}

color-display {
    border-radius: 33%;
    size: 14px 14px;
    descent: 2px;
}

control {
    size: 14px 14px;
    descent: 2px;
    background: #202024;
    border-width: 1.2px;
    border-color: #313b44;
    border-radius: 3px;
    color: #bbb;
}
control.hot {
    background: #28282c;
    border-color: #4d90fe;
}
control.depressed {
    background: #38383c;
    border-color: #4d90fe;
}

icon-button {
    size: 14px 14px;
    descent: 2px;
    background: #00000000;
    border-width: 1.2px;
    border-color: #00000000;
    border-radius: 3px;
    color: #bbb;
}
icon-button.hot {
    border-color: #4d90fe;
}
icon-button.depressed {
    background: #56565c;
    border-color: #4d90fe;
}

link {
    color: #68a;
}
link.hot {
    font-underline: true;
    color: #79b;
}
link.depressed {
    font-underline: true;
    color: #8ac;
}

node-expander.normal {
    background: #00000000;
    border-color: #00000000;
}

progress-bar {
    background: #202024;
    outline-color: #00000000;
    bar-color: #88d;
}

radio-button {
    border-radius: 50%;
}

scrollbar {
    color: #465055;
}
scrollbar.hot {
    color: #909090;
}
scrollbar.depressed {
    color: #999;
}

separator {
    color: #555;
    width: 1px;
    padding: 8px;
}

slider {
    track-color: #777;
    thumb-color: #b8b8b8;
}
slider.hot {
    thumb-color: #a8a8a8;
}
slider.depressed {
    thumb-color: #989898;
}

/* task stack */

background-task-header {
    font-size: 14;
    font-bold: true;
    color: #888;
    background: #303034;
}
background-task-header.hot {
    background: #38383c;
}
background-task-header.depressed {
    background: #404044;
}
background-task-header.selected {
    background: #667;
    color: #111;
}
background-task-header/level {
    font-family: arial black;
    font-size: 13;
    font-bold: false;
    color: #888;
}

foreground-task-header : background-task-header {
    font-size: 15;
    default-padding: 4px 4px;
    background: #889999;
    color: #000;
}   
foreground-task-header/level {
    font-family: arial black;
    font-size: 14;
    font-bold: false;
    color: #111;
}

/* used for CRADLE-level UI pages (app info, local settings, etc.) */

top-level-ui/header : content {
}
top-level-ui/header/heading {
    margin: 12px 0px 0px;
    color: #888cbc;
    font-family: georgia;
    font-size: 16;
    font-bold: true;
}

top-level-ui/content : content {
}

top-level-ui/footer : content {
}

/* used for top-level dialogs (sign in, realm selection) */

top-level-background {
    background: #252e32;
}
top-level-background/left-astroid {
    color: #2c3c43;
    gradient: #1f2629;
    scale: fill;
    alignment: right;
}
top-level-background/right-astroid : top-level-background/left-astroid {
    alignment: left;
}
top-level-background/logo {
    color: #fff;
    height: 96px;
    scale: fit;
    alignment: center;
}
top-level-dialog {
    background: #1d1e1e;
    padding: 24px 40px;
    default-padding: 4px 4px;
    border-radius: 4px;
    color: #7c8a8f;
    width: 460px;
}
top-level-dialog/animated-astroid : /animated-astroid {
    stroke-width: 0.5;
}
top-level-dialog/heading {
    color: #ffffff;
    margin: 0px 0px 8px;
    font-family: gotham SSm A;
    font-size: 26;
}
top-level-dialog/sub-heading {
    color: #ffffff;
    margin: 0px 0px 8px;
    font-family: gotham SSm A;
    font-size: 18;
}
top-level-dialog/status {
