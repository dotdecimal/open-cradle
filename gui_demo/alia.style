}

control-block/separator {
    color: #0000;
    width: 1px;
    padding: 2px;
}
control-block/content : /content {
}

control-block/inactive-header {
    margin: 0px 0px -6px 0px;
    color: #aaa;
    font-family: arial;
    font-size: 16;
