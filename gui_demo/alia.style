}
top-level-dialog/item-list {
    height: 360px;
    color: #999;
    background: #282e32;
}
top-level-dialog/item {
    background: #282e32;
    border-width: 3px;
    border-color: #1c1e1f;
    border-radius: 4px;
}
top-level-dialog/inactive-item {
    background: #282e32;
    color: #465055;
    border-radius: 4px;
}
top-level-dialog/inactive-item/heading {
    color: #465055;
    font-family: georgia;
    font-size: 14;
    font-bold: true;
}
top-level-dialog/item.hot {
    background: #2d373b;
}
top-level-dialog/item.depressed {
    background: #2c2c30;
}
top-level-dialog/item/heading {
    color: #bbb;
    font-family: georgia;
    font-size: 14;
    font-bold: true;
}
top-level-dialog/item/id {
    font-bold: true;
}
top-level-dialog/item.selected {
    /*background: #889999;*/
    /*color: #222;*/
    background: #327699;
}
top-level-dialog/control {
    background: #282e32;
    size: 14px 14px;
    descent: 2px;
    border-width: 2px;
    border-color: #282e32;
    border-radius: 3px;
    color: #bbb;
}
top-level-dialog/item.selected/heading : top-level-dialog/item/heading {
    color: #b7c4c9;
}
top-level-dialog/primary-button {
    color: #fff;
    font-bold: true;
    font-size: 13;
    padding: 2px 6px;
    /* Use a tiny amount of transparency so that text renders correctly. */
    background: #6292bdfe;
    gradient: #4e7f99fe;
    border-radius: 4px;
}
top-level-dialog/button {
    color: #ccc;
    font-bold: true;
    font-size: 13;
    padding: 2px 6px;
    background: #303232;
    border-radius: 4px;
}

/* side panel expanders */
panel-expander {
    size: 16px 16px;
    background: #1c1c20;
    color: #aaa;
    border-width: 0px;
    border-radius: 0px;
}
panel-expander.hot {
    background: #303034;
}
panel-expander.depressed {
    background: #404044;
}

/* CONTROLS SECTION - styling for content specific to the controls pane */

control-panel {
    background: #0c0c10;
}
control-panel/background {
    background: #0c0c10;
}
control-panel/content {
    background: #141418;
    padding: -4px;
}
control-panel/scrollable-content {
    background: #141418;
}

/* various common element styles */

